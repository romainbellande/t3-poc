{
  "name": "t3-poc",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "paraglide-js compile --project ./project.inlang --outdir ./src/paraglide && next build",
    "check": "next lint && tsc --noEmit",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "dev": "next dev --turbo -H t3-poc.localhost -p 80",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,mdx}\" --cache",
    "format:write": "prettier --write \"**/*.{ts,tsx,js,jsx,mdx}\" --cache",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "fix:all": "npm run format:write && npm run lint:fix",
    "preview": "next build && next start",
    "start": "next start",
    "typecheck": "tsc --noEmit",
    "authentik:setup": "open-cli http://authentik.t3-poc.localhost/if/flow/initial-setup/",
    "postinstall": "paraglide-js compile --project ./project.inlang --outdir ./src/paraglide",
    "prepare": "npx husky",
    "cy:e2e:open": "cypress open",
    "cy:e2e:run": "cypress run",
    "cy:e2e:component": "cypress run --component"
  },
  "lint-staged": {
    "**/*.{js,jsx,tsx,ts,css,less,scss,sass}": [
      "prettier --write"
    ],
    "**/*.{js,jsx,tsx,ts}": [
      "next lint --fix --file"
    ]
  },
  "dependencies": {
    "@auth/drizzle-adapter": "^1.7.2",
    "@auth/kysely-adapter": "^1.7.4",
    "@hookform/resolvers": "^4.1.0",
    "@inlang/paraglide-next": "^0.7.9",
    "@radix-ui/react-label": "^2.1.2",
    "@radix-ui/react-slot": "^1.1.2",
    "@t3-oss/env-nextjs": "^0.10.1",
    "@tailwindcss/postcss": "^4.0.6",
    "@tanstack/react-query": "^5.50.0",
    "@trpc/client": "^11.0.0-rc.446",
    "@trpc/react-query": "^11.0.0-rc.446",
    "@trpc/server": "^11.0.0-rc.446",
    "awilix": "^12.0.4",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "drizzle-orm": "^0.33.0",
    "geist": "^1.3.0",
    "kysely": "^0.26.1",
    "lucide-react": "^0.475.0",
    "next": "^15.0.1",
    "next-auth": "5.0.0-beta.25",
    "nextjs-toploader": "^3.7.15",
    "pg": "^8.13.3",
    "pool": "^0.4.1",
    "postgres": "^3.4.4",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-hook-form": "^7.54.2",
    "reflect-metadata": "^0.2.2",
    "server-only": "^0.0.1",
    "superjson": "^2.2.1",
    "tailwind-merge": "^3.0.1",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@inlang/paraglide-js": "1.11.8",
    "@types/eslint": "^8.56.10",
    "@types/node": "^20.14.10",
    "@types/pg": "^8.11.11",
    "@types/react": "^19.0.8",
    "@types/react-dom": "^19.0.3",
    "@types/webpack": "^5.28.5",
    "@typescript-eslint/eslint-plugin": "^8.1.0",
    "@typescript-eslint/parser": "^8.1.0",
    "autoprefixer": "^10.4.20",
    "cypress": "^14.0.3",
    "dotenv": "^16.4.7",
    "drizzle-kit": "^0.24.0",
    "eslint": "^9.21.0",
    "eslint-config-next": "^15.0.1",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-drizzle": "^0.2.3",
    "eslint-plugin-prettier": "^5.2.3",
    "husky": "^9.1.7",
    "lint-staged": "^15.4.3",
    "open-cli": "^8.0.0",
    "postcss": "^8.5.2",
    "prettier": "^3.5.2",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "tailwindcss": "^4.0.6",
    "typescript": "^5.5.3"
  },
  "ct3aMetadata": {
    "initVersion": "7.38.1"
  },
  "packageManager": "npm@10.9.2",
  "engines": {
    "node": ">=22.0.0"
  },
  "os": [
    "darwin",
    "linux"
  ]
}
